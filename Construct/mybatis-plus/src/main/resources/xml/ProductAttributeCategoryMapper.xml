<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gcb.learning.mybatisplus.pms.mapper.ProductAttributeCategoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.gcb.learning.mybatisplus.pms.entity.ProductAttributeCategory">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="attribute_count" property="attributeCount"/>
        <result column="param_count" property="paramCount"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, attribute_count, param_count
    </sql>

</mapper>
